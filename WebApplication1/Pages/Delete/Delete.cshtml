@page
@model WebApplication1.Pages.Delete.DeleteModel
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
}
<form method="post">
    <input hidden asp-for="registration.Id" />
    <div class="border p-3 mt-4">
        <div class="row pb-4">
            First Name:
            <input asp-for="registration.FirstName" disabled class="form-control" />
        </div>
        <div class="row pb-4">
            Last Name:
            <input asp-for="registration.LastName" disabled class="form-control" />
        </div>
        <div class="row pb-4">
            Email:
            <input asp-for="registration.Email" disabled class="form-control" />
        </div>
        <div class="row pb-4">
            Password:
            <input asp-for="registration.Password" disabled class="form-control" />
        </div>
        <button type="submit" class=" btn btn-primary" asp-page="/Delete/Delete">Delete</button>
        <a asp-page="/UserDetails/Details" class="btn btn-secondary">Back to list</a>
    </div>

</form>